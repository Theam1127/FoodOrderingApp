<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.edu.tarc.foodorderingapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MakeOrder"
            android:label="@string/app_name">

        </activity>
        <activity android:name=".MakePayment" />
        <activity android:name=".ForgotPassword" />
        <activity android:name=".ChangePassword" />
        <activity android:name=".MaintenanceSetting" />
        <activity
            android:name=".AddOrderItem"
            android:windowSoftInputMode="adjustNothing" />
        <activity android:name=".ConfirmAddOrderItem"></activity>

        <activity android:name=".GenerateReport"><intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter></activity>>
    </application>

</manifest>